def romanToInt(s):
    """
    :type s: str
    :rtype: int
    """
    convert = {'I': 1, 'V': 5, 'X': 10, 'L': 50, 'C': 100, 'D': 500, 'M': 1000}
    integer = 0
    s = s.replace('IV', 'IIII').replace('IX', 'VIIII')
    s = s.replace('XL', 'XXXX').replace('XC', 'LXXXX')
    s = s.replace('CD', 'CCCC').replace('CM', 'DCCCC')
    for symbol in s:
        integer += convert[symbol]
    return integer
s = "MCMXCIV"
print(romanToInt(s))